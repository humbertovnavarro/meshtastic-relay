// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Node {
  id BigInt @id
  shortName String?
  longName String?
  hopsAway Int?
  lastHeard Int?
  latitude Int?
  longitude Int?
  altitude Int?
  batteryLevel Int?
  voltage Int?
  channelUtilization Float?
  airUtilTx Float?
}